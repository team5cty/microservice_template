package main

import (
	"fmt"
	"net/http"
	"{{.Module}}/handlers"
	"github.com/gorilla/mux"
)


func main() {
	r := mux.NewRouter()
	{{range .Endpoints}}
	r.HandleFunc("{{.Path}}", handlers.{{.Method}}_{{.Name}}_Handler).Methods("{{.Method}}")
	{{end}}
	fmt.Println("Server is running...")
	err := http.ListenAndServe(":{{.Port}}", r)
	if err!=nil{
		fmt.Printf("Cannot start server: %s",err.Error())
	}
}